{
  "name": "ReactWeb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "i14": "ENVFILE=.env.MYT react-native run-ios --simulator \"iPhone 14 Plus\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ../..",
    "mytweb": "ENVFILE=.env.MYT mode=development webpack serve --mode=development --config webpack.config.js",
    "fhweb": "ENVFILE=.env.FH mode=development webpack serve --mode=development --config webpack.config.fh.js",
    "mytbuild": "rm -rf dist/ && ENVFILE=.env.MYT webpack --mode=production --config webpack.config.js",
    "fhbuild": "rm -rf dist/ && ENVFILE=.env.FH webpack --mode=production --config webpack.config.fh.js"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^18.4.0",
    "@react-native-firebase/app": "^18.4.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "babel-loader": "9.1.3",
    "babel-plugin-react-native-web": "0.19.8",
    "compression-webpack-plugin": "10.0.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.3",
    "react": "18.0.0",
    "react-dom": "18.2.0",
    "react-native": "0.69.0",
    "react-native-config": "1.4.11",
    "react-native-gesture-handler": "1.10.3",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-web": "0.19.8",
    "react-native-web-webview": "1.0.2",
    "react-native-webview": "11.24.0",
    "react-web-config": "https://github.com/Hortau/react-web-config.git#webpack5",
    "url-loader": "4.1.1",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/runtime": "7.12.5",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/react": "18.2.21",
    "@types/react-native": "0.72.2",
    "babel-jest": "26.6.3",
    "eslint": "7.32.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.70.3",
    "react-test-renderer": "18.0.0",
    "ts-loader": "9.4.4",
    "typescript": "5.2.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
